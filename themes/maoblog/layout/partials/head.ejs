<head>
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no, viewport-fit=cover" name="viewport" />
  <meta name="description" content="<%= page.description ? page.description : config.description %>">
  <meta name="keywords" content="<%= page.keywords ? page.keywords.join(', ') : config.keywords %>">

  <link rel="alternate icon" type="image/png" href="<%- url_for('/img/favicon.png') %>">
  <link rel="apple-touch-icon" sizes="180x180" href="<%- url_for('/img/apple-touch-icon.png') %>?v=1">
  <link rel="apple-touch-icon" sizes="152x152" href="<%- url_for('/img/apple-touch-icon-152x152.png') %>?v=1">
  <link rel="apple-touch-icon" sizes="120x120" href="<%- url_for('/img/apple-touch-icon-120x120.png') %>?v=1">
  
  <title>
    <% if (page.title) { %>
      <%= page.title %> |
    <% } %> 
    <%= config.title %>
  </title>
  <%- css('css/maoblog') %>
  <%- css('css/partials/post') %>
  <%- css('css/partials/comment') %>
  <%- css('css/partials/search') %>
  <%- css('css/partials/meta') %>
  <%- css('css/partials/about') %>
  <%- css('fontawesome/css/font-awesome.min') %>
  <%- css('css/zoom.css') %>
  <%- css('css/prism.css') %>
</head>
